{
  "name": "backend",
  "version": "1.0.0",
  "main": "src/index.ts",
  "scripts": {
    "dev": "npx dotenv -e .env.dev -- nodemon src/index.ts",
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "serve": "node dist/index.js",
    "lint": "eslint .",
    "test": "dotenv -e .env.test -- jest --runInBand",
    "migrate:test": "dotenv -e .env.test -- npx prisma migrate dev",
    "migrate:dev": "dotenv -e .env.dev -- npx prisma migrate dev",
    "reset:dev": "dotenv -e .env.dev -- npx prisma migrate reset",
    "reset:test": "dotenv -e .env.test -- npx prisma migrate reset",
    "seed": "ts-node prisma/seed.ts",
    "seed:dev": "dotenv -e .env.dev -- ts-node prisma/seedDev.ts",
    "seed:test": "dotenv -e .env.test -- ts-node prisma/seed.ts"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.15.27",
    "@types/supertest": "^6.0.3",
    "dotenv": "^16.5.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.26.0",
    "globals": "^16.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.7.0",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1"
  },
  "dependencies": {
    "@prisma/client": "^6.7.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "js-combinatorics": "^2.1.2",
    "jsonwebtoken": "^9.0.2"
  }
}
